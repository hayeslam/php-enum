    "type": "library",
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "myclabs/php-enum": "^1.8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
